<configuration>
  <configSections>
    <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 -->
    <section name="entityFramework" type="System.Data.Entity.Internal.ConfigFile.EntityFrameworkSection, EntityFramework, Version=6.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    <section name="log4net" type="System.Configuration.IgnoreSectionHandler" />
  </configSections>
  <connectionStrings>
    <add name="TPOEntities" connectionString="metadata=res://*/TPO.Entity.Data.csdl|res://*/TPO.Entity.Data.ssdl|res://*/TPO.Entity.Data.msl;provider=System.Data.SqlClient;provider connection string=&quot;data source=eimagine-sql14.eimagine.local;initial catalog=TPOMVCApplication;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework&quot;" providerName="System.Data.EntityClient" />
    <add name="SecurityService" connectionString="data source=eimagine-sql14.eimagine.local;initial catalog=TPOMVCApplication;integrated security=True;MultipleActiveResultSets=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="enableSimpleMembership" value="true" />
    <add key="ResourceNamespace" value="TPO.Web.Content.Resources" />
    <add key="MessageResources" value="Shoals General" />
    <!-- for debugging log4net -->
    <add key="log4net.Internal.Debug" value="true" />
    <!-- for debugging log4net -->
    <add key="log4net.Internal.Debug" value="true" />
    <add key="loginUrl" value="/Security/Login" />
    <add key="loadDefaultUsers" value="true" />
    <add key="defaultPassword" value="think4TPO!" />
    <add key="defaultPlantId" value="1" />
  </appSettings>
  <!-- for debugging log4net -->
  <system.diagnostics>
    <trace autoflush="true">
      <listeners>
        <add name="textWriterTraceListener" type="System.Diagnostics.TextWriterTraceListener" initializeData="C:\tmp\log4net.txt" />
      </listeners>
    </trace>
  </system.diagnostics>
